{
  "name": "back",
  "version": "0.0.0",
  "description": "Backend for SuperFeed",
  "repository": "https://github.com/SuperFeed/Back",
  "main": "index.js",
  "scripts": {
    "test": "standard | snazzy",
    "predev": "babel src -d back",
    "dev": "parallelshell \"babel --watch src -d back\" \"nodemon --watch back back/server.js\"",
    "build": "babel src -d .",
    "postbuild": "rimraf src"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "migrations/",
      "seeds/"
    ]
  },
  "author": "Ryan Delaney <rrdelaney@outlook.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-eslint": "^5.0.0",
    "babel-plugin-transform-async-to-generator": "^6.5.0",
    "babel-plugin-transform-decorators": "^6.5.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015-node5": "^1.1.2",
    "babel-preset-stage-0": "^6.5.0",
    "cross-env": "^1.0.7",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.9.0",
    "parallelshell": "^2.0.0",
    "snazzy": "^3.0.0",
    "sqlite3": "^3.1.1",
    "standard": "^5.4.1"
  },
  "dependencies": {
    "kcors": "^2.0.0",
    "knex": "^0.10.0",
    "koa": "^2.0.0-alpha.3",
    "koa-bodyparser": "^3.0.0",
    "koa-passport": "^2.1.0",
    "koa-route": "^3.1.0",
    "mysql": "^2.10.2",
    "npmlog": "^2.0.2",
    "passport-facebook": "^2.1.0"
  }
}
